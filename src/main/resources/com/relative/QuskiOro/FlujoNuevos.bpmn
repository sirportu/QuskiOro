//SETEAMOS LOS VALORES POR DEFECTO DE LOS MENSAJES DE ERROR
simularResultAux.setCodigoError(variablesNegocio.getCodigoError().equals("0") ? variablesNegocio.getCodigoMensajeErrorConstante() : variablesNegocio.getCodigoError());
simularResultAux.setMensaje(variablesNegocio.getMensaje().equals("") ? variablesNegocio.getMensajeError() : variablesNegocio.getMensaje());
simularResultAux.setNumeroOperacionMadre("");

//DECLARACION DE XMLVARIABLESINTERNAS
com.relative.QuskiOro.ObjetosNegocio.XmlVariablesInternasSalida variablesInternas = new com.relative.QuskiOro.ObjetosNegocio.XmlVariablesInternasSalida();
variablesInternas.setVariablesInternas(new com.relative.QuskiOro.ObjetosNegocio.VariablesInternasSalida());
variablesInternas.getVariablesInternas().setVariable(new java.util.ArrayList<com.relative.QuskiOro.ObjetosNegocio.Variable>());
com.relative.QuskiOro.ObjetosNegocio.Variable variableAux = new com.relative.QuskiOro.ObjetosNegocio.Variable();
int cont = 0;
double auxDob=0.0;
int auxDob2=0;

//LLENAMOS LA LISTA DE VARIABLES INTERNAS A DEVOLVER
if(cliente.getTipo() != null){
    cont++;
    variableAux = new com.relative.QuskiOro.ObjetosNegocio.Variable("Tipo Cliente",cont,cliente.getTipo(),"TipoCliente");
    variablesInternas.getVariablesInternas().getVariable().add(variableAux);
}

if(cliente.getPerfilExterno() != null){
    cont++;
    variableAux = new com.relative.QuskiOro.ObjetosNegocio.Variable("Perfil Externo",cont,cliente.getPerfilExterno(),"PerfilExterno");
    variablesInternas.getVariablesInternas().getVariable().add(variableAux);
}

if(cliente.getTipo().equals("Q")){
    if(cliente.getPerfilComportamiento() != null){
        cont++;
        variableAux = new com.relative.QuskiOro.ObjetosNegocio.Variable("Perfil Comportamiento",cont,cliente.getPerfilComportamiento().toString(), "PerfilComportamiento");
        variablesInternas.getVariablesInternas().getVariable().add(variableAux);
    }

    if(cliente.getPerfilInterno() != null){
        cont++;
        variableAux = new com.relative.QuskiOro.ObjetosNegocio.Variable("Perfil Interno",cont,cliente.getPerfilInterno(), "PerfilInterno");
        variablesInternas.getVariablesInternas().getVariable().add(variableAux);
    }
}

if(cliente.getMontoCarteraVencida() != null){
    cont++;
    variableAux = new com.relative.QuskiOro.ObjetosNegocio.Variable("Cart. Vencida 90D",cont,cliente.getMontoCarteraVencida().toString(), "CartVencida90D");
    variablesInternas.getVariablesInternas().getVariable().add(variableAux);
}

if(cliente.getMontoCarteraCastigada() != null){
    cont++;
    variableAux = new com.relative.QuskiOro.ObjetosNegocio.Variable("Cart. Castigada",cont,cliente.getMontoCarteraCastigada().toString(), "CartCastigada");
    variablesInternas.getVariablesInternas().getVariable().add(variableAux);
}

if(Carga.getXmlVariablesInternas().getVariablesInternas().getRiesgoAcumulado() != null){
    cont++;
    Double riesgoTotal = ((double)Math.round((Carga.getXmlVariablesInternas().getVariablesInternas().getRiesgoAcumulado() + variablesNegocio.getTotalGarantia())*100)/100);
    variableAux = new com.relative.QuskiOro.ObjetosNegocio.Variable("Riesgo acumulado",cont,riesgoTotal.toString(), "RiesgoAcumulado");
    variablesInternas.getVariablesInternas().getVariable().add(variableAux);
}

if(cliente.getCategoria() != null){
    cont++;
    variableAux = new com.relative.QuskiOro.ObjetosNegocio.Variable("Categoría",cont,cliente.getCategoria(), "Categoria");
    variablesInternas.getVariablesInternas().getVariable().add(variableAux);
}

if(cliente.getColateral() != null){
    cont++;
    variableAux = new com.relative.QuskiOro.ObjetosNegocio.Variable((cliente.getColateralExcepcionada() > 0 ? cliente.getColateralOriginal() : cliente.getColateral()) > 100  ? "Cobertura" : "Descobertura",cont,(cliente.getColateralExcepcionada() > 0 ? cliente.getColateralOriginal() : cliente.getColateral()).toString(),"Cobertura");
    variablesInternas.getVariablesInternas().getVariable().add(variableAux);
}

if(cliente.getColateralExcepcionada() != null){
    cont++;
        variableAux = new com.relative.QuskiOro.ObjetosNegocio.Variable("Cobertura excepcionada",cont,cliente.getColateralExcepcionada().toString(), "CoberturaExcepcionada");
        variablesInternas.getVariablesInternas().getVariable().add(variableAux);
}

if(cliente.getTipo().equals("Q")){
    if(cliente.getPermanencia() != null){
        cont++;
        variableAux = new com.relative.QuskiOro.ObjetosNegocio.Variable("Permanencia",cont,cliente.getPermanencia().toString(), "Permanencia");
        variablesInternas.getVariablesInternas().getVariable().add(variableAux);
    }

    if(cliente.getDiasMoraMaximo() != null){
        cont++;
        variableAux = new com.relative.QuskiOro.ObjetosNegocio.Variable("Mora Máximo",cont,cliente.getDiasMoraMaximo().toString(), "MoraMaximo");
        variablesInternas.getVariablesInternas().getVariable().add(variableAux);
    }
    
    if(cliente.getDiasMoraPromedio() != null){
        cont++;
        variableAux = new com.relative.QuskiOro.ObjetosNegocio.Variable("Mora Promedio",cont,cliente.getDiasMoraPromedio().toString(), "MoraPromedio");
        variablesInternas.getVariablesInternas().getVariable().add(variableAux);
    }
    
    if(cliente.getNumeroCreditos() != null){
        cont++;
        Integer nCreditos = cliente.getNumeroCreditos() + 1;
        variableAux = new com.relative.QuskiOro.ObjetosNegocio.Variable("Créditos Vigentes",cont,nCreditos.toString(), "CreditosVigentes");
        variablesInternas.getVariablesInternas().getVariable().add(variableAux);
    }
    
    if(cliente.getTotalCreditos() != null){
        cont++;
        variableAux = new com.relative.QuskiOro.ObjetosNegocio.Variable("Créditos Cancelados",cont,cliente.getTotalCreditos().toString(), "CreditosCancelados");
        variablesInternas.getVariablesInternas().getVariable().add(variableAux);
    }
    
    if(cliente.getNumeroPagos() != null){
        cont++;
        variableAux = new com.relative.QuskiOro.ObjetosNegocio.Variable("Pagos",cont,cliente.getNumeroPagos().toString(), "Pagos");
        variablesInternas.getVariablesInternas().getVariable().add(variableAux);
    }
}

//if(campaña == null){
    cont++;
    variableAux = new com.relative.QuskiOro.ObjetosNegocio.Variable("Campaña",cont,"N", "Campaña");
    variablesInternas.getVariablesInternas().getVariable().add(variableAux);
//}

simularResultAux.setXmlVariablesInternas(variablesInternas);

//TERMINAMOS DE LLENAR TODO EL OBJETO PRINCIPAL
System.out.println("<<<Cantidad de errores: "+variablesNegocio.getCodigoPolitica().size()+">>>");
System.out.println("<<<Cantidad de plazos finales: "+opcionRenovacion.getOpcion().size()+">>>");


