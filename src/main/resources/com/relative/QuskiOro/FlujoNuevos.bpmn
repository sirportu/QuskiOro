      <bpmn2:script><![CDATA[//INSTANCIAMOS EL OBJETO QUE ENVIARA TODO EL OUTPUT
com.relative.QuskiOro.ObjetosNegocio.SimularResult simularResultGarantias = new com.relative.QuskiOro.ObjetosNegocio.SimularResult();

//SETEAMOS LOS VALORES POR DEFECTO DE LOS MENSAJES DE ERROR
simularResultGarantias.setCodigoError(variablesNegocio.getCodigoError().equals("0") ? variablesNegocio.getCodigoMensajeErrorConstante() : variablesNegocio.getCodigoError());
simularResultGarantias.setMensaje(variablesNegocio.getMensaje().equals("") ? variablesNegocio.getMensajeError() : variablesNegocio.getMensaje());
simularResultGarantias.setNumeroOperacionMadre("");

//TERMINAMOS DE LLENAR TODO EL OBJETO PRINCIPAL
System.out.println("<<<Cantidad de errores: "+variablesNegocio.getCodigoPolitica().size()+">>>");
System.out.println("<<<Cantidad de plazos finales: "+opcionRenovacion.getOpcion().size()+">>>");

simularResultGarantias.setXmlGarantias(Carga.getXmlGarantias());

//MAPEAMOS TODOS LOS DATOS OBTENIDOS EN EL PROCESO
com.fasterxml.jackson.databind.ObjectWriter ow = new com.fasterxml.jackson.databind.ObjectMapper().writer().withDefaultPrettyPrinter();
String json = ow.writeValueAsString(simularResultGarantias);
System.out.println(json);
org.json.JSONObject jsonOb = new org.json.JSONObject(json);
String xml = org.json.XML.toString(jsonOb);
System.out.println(xml);
salida = xml;
kcontext.setVariable("salida",salida);]]></bpmn2:script>
