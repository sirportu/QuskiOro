    <bpmn2:scriptTask id="_84EEAC1A-609B-4D2F-90ED-19CC183B79A5" name="Calculo
ServiciosTask" scriptFormat="http://www.java.com/java">
      <bpmn2:extensionElements>
        <drools:metaData name="elementname">
