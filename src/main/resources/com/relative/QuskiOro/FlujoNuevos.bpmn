      <bpmn2:script><![CDATA[//IMPRIMIMOS EL PERIODO DEL PLAZO EN DIAS PARA CREDITOS DIFERIDOS Y EN MESES PARA CUOTAS
System.out.println("<<<< PeriodoPlazo:"+opcion.getPeriodoPlazo()+">>>>");
System.out.println("<<<< codigo error politica:"+variablesNegocio.getCodigoPolitica()+">>>>");
if(opcion.getPeriodoPlazo().equals(kcontext.getKnowledgeRuntime().getGlobal("CodigoCreditoDiferido"))){
    System.out.println("<<<< Cambio Plazo:"+opcion.getPlazo()+">>>>");
    opcion.setPlazo(opcion.getPlazo() * 30);
}

//SETEAMOS EN EL OBJETO LA OPCION DE RENOVACION GESTIONADA
opcionRenovacion.getOpcion().set( (index-1), opcion );]]></bpmn2:script>
