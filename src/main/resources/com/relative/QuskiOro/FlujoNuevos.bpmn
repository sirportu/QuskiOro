      <bpmn2:script><![CDATA[//INSTANCIAMOS EL OBJETO QUE ENVIARA TODO EL OUTPUT
com.relative.QuskiOro.ObjetosNegocio.SimularResultSimulation simularResultSimulation = new com.relative.QuskiOro.ObjetosNegocio.SimularResultSimulation();

//SETEAMOS LOS VALORES POR DEFECTO DE LOS MENSAJES DE ERROR
simularResultSimulation.setCodigoError(variablesNegocio.getCodigoError().equals("0") ? variablesNegocio.getCodigoMensajeErrorConstante() : variablesNegocio.getCodigoError());
simularResultSimulation.setMensaje(variablesNegocio.getMensaje().equals("") ? variablesNegocio.getMensajeError() : variablesNegocio.getMensaje());
simularResultSimulation.setNumeroOperacionMadre("");

//TERMINAMOS DE LLENAR TODO EL OBJETO PRINCIPAL
System.out.println("<<<Cantidad de errores: "+variablesNegocio.getCodigoPolitica().size()+">>>");
System.out.println("<<<Cantidad de plazos finales: "+opcionRenovacion.getOpcion().size()+">>>");

//CREAMOS EL OBJETO DE SIMULACION BASE A LAS OPCIONES DE CREDITOS OBTENIDAS
java.util.ArrayList<com.relative.QuskiOro.ObjetosNegocio.OpcionSimulada> listaOpcionSimulada = new java.util.ArrayList<com.relative.QuskiOro.ObjetosNegocio.OpcionSimulada>();
com.relative.QuskiOro.ObjetosNegocio.OpcionSimulada opcionSimulada = new com.relative.QuskiOro.ObjetosNegocio.OpcionSimulada();
simularResultSimulation.setXmlOfertasSimuladas(new com.relative.QuskiOro.ObjetosNegocio.XmlOfertasSimuladas());
simularResultSimulation.getXmlOfertasSimuladas().setOfertasSimuladas(new com.relative.QuskiOro.ObjetosNegocio.OfertasSimuladas());

if(variablesNegocio.getTotalGarantia() > 0 && simularResultSimulation.getMensaje().equals("")){
    Double riesgoTotal = ((double)Math.round((Carga.getXmlVariablesInternas().getVariablesInternas().getRiesgoAcumulado() + variablesNegocio.getTotalGarantia())*100)/100);

    for(com.relative.QuskiOro.ObjetosNegocio.Opcion opcionAux : opcionRenovacion.getOpcion()){
        opcionSimulada = new com.relative.QuskiOro.ObjetosNegocio.OpcionSimulada(
            opcionAux.getPlazo(),
            (double)Math.round(opcionAux.getMontoFinanciado()*100)/100,
            riesgoTotal,
            (double)Math.round(opcionAux.getValorARecibir()*100)/100,
            (double)Math.round(opcionAux.getCuota()*100)/100
        );

        listaOpcionSimulada.add(opcionSimulada);
    }
}else{
    opcionSimulada = new com.relative.QuskiOro.ObjetosNegocio.OpcionSimulada(
        null,
        null,
        null,
        null,
        null
    );

    listaOpcionSimulada.add(opcionSimulada);
}

simularResultSimulation.getXmlOfertasSimuladas().getOfertasSimuladas().setOpcion(listaOpcionSimulada);

//MAPEAMOS TODOS LOS DATOS OBTENIDOS EN EL PROCESO
com.fasterxml.jackson.databind.ObjectWriter ow = new com.fasterxml.jackson.databind.ObjectMapper().writer().withDefaultPrettyPrinter();
String json = ow.writeValueAsString(simularResultSimulation);
System.out.println(json);
org.json.JSONObject jsonOb = new org.json.JSONObject(json);
String xml = org.json.XML.toString(jsonOb);
System.out.println(xml);
salida = xml;
kcontext.setVariable("salida",salida);]]></bpmn2:script>
