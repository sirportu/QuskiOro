      <bpmn2:script><![CDATA[//GESTIONAMOS TODOS LSO COSTOS DE LOS SERVICIOS
double decimal = 0.00;
System.out.println("<<<Plazo: "+opcion.getPlazo()+">>>");
System.out.println(variablesNegocio.getTipoOperacion());
double seguroM4 = opcion.getCostoValoracion();
opcion.setCostoTasacion((double)Math.round((((opcion.getCostoTasacion()*cliente.getTasacion())*variablesNegocio.getTotalGarantia())/100)*100)/100);
System.out.println("<<<Tasacion: "+opcion.getCostoTasacion()+">>>");
opcion.setCostoTransporte((double)Math.round((((opcion.getCostoTransporte()*cliente.getTransporte())*variablesNegocio.getTotalGarantia())/100)*100)/100);
System.out.println("<<<Transporte: "+opcion.getCostoTransporte()+">>>");
opcion.setCostoCustodia((double)Math.round((((opcion.getCostoCustodia()*cliente.getCustodia())*variablesNegocio.getTotalGarantia())/100)*100)/100);
System.out.println("<<<Custodia: "+opcion.getCostoCustodia()+">>>");
opcion.setCostoFideicomiso((double)Math.round((((opcion.getCostoFideicomiso()*cliente.getResguardo())*variablesNegocio.getTotalGarantia())/100)*100)/100);
System.out.println("<<<Resguardo: "+opcion.getCostoFideicomiso()+">>>");

//SI EL CREDITO ES MAYOR A 4000 USAMOS SEGUROM4, SINO SOLO SEGURO
if(variablesNegocio.getTotalGarantia() > 4000){
    System.out.println("<<< El monto del credito exede los 4000$  ("+opcion.getCostoSeguro()+", "+seguroM4+")   >>>");
    double seg = (opcion.getCostoSeguro()/100)*4000;
    double segM4 = (seguroM4/100)*(variablesNegocio.getTotalGarantia()-4000);
    opcion.setCostoSeguro((double)Math.round((seg+segM4)*100)/100);
}else{
    opcion.setCostoSeguro((double)Math.round(((opcion.getCostoSeguro()*variablesNegocio.getTotalGarantia())/100)*100)/100);
}

//IMPRIMIMOS Y SETEAMOS CADA UNO DE LOS VALORES DE LOS SERVICIOS
System.out.println("<<<Seguro: "+opcion.getCostoSeguro()+">>>");
System.out.println("<<<CostoValoracion: "+opcion.getCostoValoracion()+">>>");
opcion.setCostoValoracion(decimal);
variablesNegocio.setTotalServiciosCapitalizados(decimal);
variablesNegocio.setTotalServiciosDiferidos(decimal);
opcion.setMontoFinanciado(variablesNegocio.getTotalGarantia());
System.out.println("<<<Monto Financiado: "+opcion.getMontoFinanciado()+">>>");
opcion.setTotalGastosNuevaOperacion((double)Math.round((opcion.getCostoTasacion()+opcion.getCostoTransporte()+opcion.getCostoCustodia()+opcion.getCostoFideicomiso()+opcion.getCostoSeguro())*100)/100);
System.out.println("<<<Total gastos nueva operacion: "+opcion.getTotalGastosNuevaOperacion()+">>>");
System.out.println("<<<Tipo de formaCustodia: "+opcion.getFormaPagoCustodia()+">>>");

if(opcion.getFormaPagoCustodia().equals(kcontext.getKnowledgeRuntime().getGlobal("FormaPagoCapitalizado"))){
    variablesNegocio.setTotalServiciosCapitalizados((double)Math.round((variablesNegocio.getTotalServiciosCapitalizados()+opcion.getCostoCustodia())*100)/100);
}
else{
    variablesNegocio.setTotalServiciosDiferidos((double)Math.round((variablesNegocio.getTotalServiciosDiferidos()+opcion.getCostoCustodia())*100)/100);
}

System.out.println("<<<Total Servicios Diferido: "+variablesNegocio.getTotalServiciosDiferidos()+">>>");
variablesNegocio.setTotalServiciosCapitalizados((double)Math.round((variablesNegocio.getTotalServiciosCapitalizados()+opcion.getCostoTasacion()+opcion.getCostoTransporte()+opcion.getCostoFideicomiso()+opcion.getCostoSeguro())*100)/100);
System.out.println("<<<Total Servicios Capitalizados: "+variablesNegocio.getTotalServiciosCapitalizados()+">>>");
opcion.setValorARecibir((double)Math.round((opcion.getMontoFinanciado()-variablesNegocio.getTotalServiciosCapitalizados()-variablesNegocio.getTotalServiciosDiferidos())*100)/100);
System.out.println("<<<Valor a recibir: "+opcion.getValorARecibir()+">>>");
opcion.setValorAPagar(decimal);
System.out.println("<<<Valor a pagar: "+opcion.getValorAPagar()+">>>");
opcion.setSaldoInteres(decimal);
System.out.println("<<<Saldo interes: "+opcion.getSaldoInteres()+">>>");
opcion.setSaldoMora(decimal);
System.out.println("<<<Saldo mora: "+opcion.getSaldoMora()+">>>");
opcion.setGastoCobranza(decimal);
System.out.println("<<<Gasto cobranza: "+opcion.getGastoCobranza()+">>>");
opcion.setSaldoCapitalRenov(decimal);
System.out.println("<<<Saldo capital renv: "+opcion.getSaldoCapitalRenov()+">>>");
opcion.setMontoPrevioDesembolso(decimal);
System.out.println("<<<Monto previo desembolso: "+opcion.getMontoPrevioDesembolso()+">>>");
opcion.setTotalCostosOperacionAnterior(decimal);
System.out.println("<<<Total costos operacione anterior: "+opcion.getTotalCostosOperacionAnterior()+">>>");
System.out.println("<<<detener: "+variablesNegocio.getDetenerColateral()+">>>");
opcion.setCustodiaDevengada(decimal);
System.out.println("<<<Custodia Devengada: "+opcion.getCustodiaDevengada()+">>>");
opcion.setFormaPagoCustodiaDevengada("C");
System.out.println("<<<Forma Pago Custodia Devengada: "+opcion.getFormaPagoCustodiaDevengada()+">>>");

//double MontoCalculado = 0.00;
double Interes = 0.00;
double CuotaPrevia = 0.00;
double DiferidosCuota = 0.00;
if (opcion.getTIPO_OFERTA().equals(kcontext.getKnowledgeRuntime().getGlobal("TipoOfertaBalloon")))
{
    /*MontoCalculado=((opcion.getMontoFinanciado()*opcion.getPORCENTAJE_FLUJO_PLANEADO())/100)+variablesNegocio.getTotalServiciosDiferidos(); 
    opcion.setCuota((double)Math.round((MontoCalculado/(opcion.getPlazo()-1)*100)/100));*/

    System.out.println("<<<Porcentaje flujo planeado Baloom: "+opcion.getPORCENTAJE_FLUJO_PLANEADO()+">>>");
    DiferidosCuota = variablesNegocio.getTotalServiciosDiferidos() / (opcion.getPlazo() -1);
    System.out.println("<<<DiferidosCuota Baloom: "+DiferidosCuota+">>>");
    CuotaPrevia = ((opcion.getMontoFinanciado() * (100-opcion.getPORCENTAJE_FLUJO_PLANEADO()))/100)/(opcion.getPlazo() -1);
    System.out.println("<<<CuotaPrevia Baloom: "+CuotaPrevia+">>>");
    Interes = (opcion.getMontoFinanciado() *(((Double)kcontext.getKnowledgeRuntime().getGlobal("TasaInteres"))/100/12*opcion.getPlazo()))/(opcion.getPlazo() -1);
    System.out.println("<<<Interes Baloom: "+Interes+">>>");
    opcion.setCuota((double)Math.round(((( DiferidosCuota + Interes + CuotaPrevia))*100)/100));
    System.out.println("<<<Cuota Baloom: "+opcion.getCuota()+">>>");
    opcion.setMontoPrevioDesembolso((double)Math.round(((opcion.getMontoFinanciado()-variablesNegocio.getTotalServiciosCapitalizados())*100)/100));
    System.out.println("<<<Monto previo desembolso Baloom: "+opcion.getMontoPrevioDesembolso()+">>>");
}else{
    //CALCULAMOS LAS CUOTAS DEPENDIENDO DEL TIPO DE CREDITO (CUOTA, DIFERIDO)
    if(opcion.getPeriodoPlazo().equals(kcontext.getKnowledgeRuntime().getGlobal("CodigoCreditoCuota"))){
        opcion.setCuota((double)Math.round((opcion.getMontoFinanciado()*(Math.pow((1+(((Double)kcontext.getKnowledgeRuntime().getGlobal("TasaInteres")/100)/12)),opcion.getPlazo()) * (((Double)kcontext.getKnowledgeRuntime().getGlobal("TasaInteres")/100)/12))/(Math.pow((1+(((Double)kcontext.getKnowledgeRuntime().getGlobal("TasaInteres")/100)/12)),opcion.getPlazo()) - 1))*100)/100);
    }else{
        opcion.setCuota((double)Math.round((opcion.getMontoFinanciado()*(1+((((Double)kcontext.getKnowledgeRuntime().getGlobal("TasaInteres"))/100)/12)*opcion.getPlazo()))*100)/100);
    }
}

//IMPRIMIMOS LA CUOTA
System.out.println("<<<Cuota: "+opcion.getCuota()+">>>");

//IMPRIMIMOS COLATERAL ADICIONAL PREVIO PO3
System.out.println("<<<PREVIO A POLITICA 3 - calculo servicios task>>>");
System.out.println("<<<Colateral adicional: "+cliente.getColateralAdicional()+">>>");
System.out.println("<<<Codigo ciudad ( 025,030 ): "+cliente.getCiudadProceso()+">>>");
System.out.println("<<<riesgo total (>10k) : "+cliente.getRiesgoTotal()+">>>");
System.out.println("<<<total garantia (>5k): "+cliente.getRiesgoTotal()+">>>");
]]></bpmn2:script>
