System.out.println("<<<Codigos errores generales: "+variablesNegocio.getCodigoMensajeError()+">>>");
System.out.println("<<<Mensajes errores generales: "+variablesNegocio.getMensajeError()+">>>");

//SI NO PASO LA POLITICA DE CLIENTEBLOQUEADO GESTIONAMOS PARA AGREGARLAS A LA SALIDA
if(variablesNegocio.getCodigoPoliticaBloqueo().equals(kcontext.getKnowledgeRuntime().getGlobal("ClienteBloqueado"))){
    System.out.println("<<<ENTRAMOS EN POLITICA DE BLOQUEO>>>");
    //SI NO HAY MAS CODIGOS AÑADIMOS ESTE SOLO, SINO CONCATENAMOS
    if(variablesNegocio.getCodigoError().equals("0")){
        variablesNegocio.setMensaje("Bloqueo: "+cliente.getBloqueosCliente());
    }else{
        variablesNegocio.setMensaje(variablesNegocio.getMensaje()+"^;Bloqueo: "+cliente.getBloqueosCliente());
    } 
    variablesNegocio.setCodigoError((String)kcontext.getKnowledgeRuntime().getGlobal("ClienteBloqueadoOutput"));
}

variablesNegocio.setMensaje(variablesNegocio.getMensaje().replace("^;&&",""));
variablesNegocio.setMensaje(variablesNegocio.getMensaje().replace("^~&&",""));
variablesNegocio.setMensaje(variablesNegocio.getMensaje().replace("&&",""));

System.out.println("<<<Errores concatenados: "+variablesNegocio.getMensaje()+">>>");

java.util.ArrayList<String> values = new java.util.ArrayList<String>(java.util.Arrays.asList(variablesNegocio.getMensaje().split("\\^")));
java.util.HashSet<String> uniqueValues = new java.util.HashSet<>(values);
variablesNegocio.setMensaje(java.lang.String.join("^", uniqueValues));

System.out.println("<<<Errores concatenado2: "+variablesNegocio.getMensaje()+">>>");

variablesNegocio.setMensaje(variablesNegocio.getMensaje().replace("^",""));
variablesNegocio.setMensaje(variablesNegocio.getMensaje().replace(";","\n"));
variablesNegocio.setMensaje(variablesNegocio.getMensaje().replace("~"," "));

System.out.println("<<<Errores concatenados3: "+variablesNegocio.getMensaje()+">>>");]]></bpmn2:script>
