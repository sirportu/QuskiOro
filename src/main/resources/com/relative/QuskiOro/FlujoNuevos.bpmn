    <bpmn2:scriptTask id="_9E665917-234C-461F-9EAC-83D45417BAD2" name="CalculoPrecioTask" scriptFormat="http://www.java.com/java">
      <bpmn2:extensionElements>
        <drools:metaData name="elementname">
          <drools:metaValue><![CDATA[CalculoPrecioTask]]></drools:metaValue>
        </drools:metaData>
      </bpmn2:extensionElements>
      <bpmn2:incoming>_AA3679D0-50A9-4CD8-ACDD-ABE7F2408B47</bpmn2:incoming>
      <bpmn2:outgoing>_1B9D5D2C-9293-4C38-8344-34E7A1A03F0A</bpmn2:outgoing>
      <bpmn2:script><![CDATA[//SETEAMOS EL PRECIO DEL ORO EN BASE A LO OBTENIDO EN LA MATRIZ PRECIO
System.out.println("<<<PrecioOro matriz: "+garantia.getValorAvaluo()+">>>");
System.out.println("<<<Oro 100 por ciento: "+kcontext.getKnowledgeRuntime().getGlobal("Oro100Porciento")+">>>");
garantia.setValorAvaluo(garantia.getValorAvaluo() / ((Integer)kcontext.getKnowledgeRuntime().getGlobal("Oro100Porciento")));
System.out.println("<<<PrecioOro 2 : "+garantia.getValorAvaluo()+">>>");
garantia.setValorOro(garantia.getValorAvaluo()*variablesNegocio.getPrecioGramo());
garantia.setValorAvaluo((double)Math.round((garantia.getValorAvaluo()*(variablesNegocio.getPrecioGramo()*garantia.getPesoNeto()))*100)/100);
System.out.println("<<<Precio gramo : "+variablesNegocio.getPrecioGramo()+">>>");
System.out.println("<<<Peso neto : "+garantia.getPesoNeto()+">>>");
System.out.println("<<<PrecioOro 3 : "+garantia.getValorAvaluo()+">>>");

variablesNegocio.setTotalPrecioOro(variablesNegocio.getTotalPrecioOro()+garantia.getValorAvaluo());

//SI NO HAY COLATERAL DEVOLVEMOS 0 EN VALORAPLICABLECREDITO Y VALOR REALIZACION, SINO CALCULAMOS
if(cliente.getColateral() == 0){
    garantia.setValorAplicableCredito(0.00);
    garantia.setValorRealizacion(0.00);
    garantia.setValorOro(0.00);
}else{
    garantia.setValorAplicableCredito((double)Math.round((garantia.getValorAvaluo()/(cliente.getColateral()/100.00))*100)/100);
    System.out.println("valor Oro"+garantia.getValorOro());
    System.out.println("valor colateral"+cliente.getColateral());
    System.out.println("valor calculado colateral"+(cliente.getColateral()/100.00));
    double decimal=(garantia.getValorOro()/(cliente.getColateral()/100.00));
    System.out.println("valor calculado oro/colateral"+decimal);
    java.math.BigDecimal bdTest = new java.math.BigDecimal(decimal);
    bdTest = bdTest.setScale(2, java.math.BigDecimal.ROUND_HALF_UP);
    System.out.println("valor calculado oro/colateral"+bdTest);
    garantia.setValorOro(bdTest.doubleValue());
    //double segundoCalculo=(double)Math.round((garantia.getValorOro()/(cliente.getColateral()/100.00))*100)/100;
    //System.out.println("Segundo calculo"+segundoCalculo);
    garantia.setValorRealizacion((double)Math.round((garantia.getValorAvaluo()*((Double)kcontext.getKnowledgeRuntime().getGlobal("MultiploRealizacion")))*100)/100);
}

//ACUMULAMOS EL TOTAL GARANTIA Y TOTAL PESO
variablesNegocio.setTotalGarantia((double)Math.round((variablesNegocio.getTotalGarantia()+garantia.getValorAplicableCredito())*100)/100);
variablesNegocio.setTotalPeso((double)Math.round((variablesNegocio.getTotalPeso() + garantia.getPesoNeto())*100)/100);

//SETEAMOS EL OBJETO GARANTIA DEL OBJETO PRINCIPAL, YA QUE SE TIENE QUE DEVOLVER CON OTRA INFORMACION
System.out.println("<<<ValorAvaluo: "+garantia.getValorAvaluo()+">>>");
System.out.println("<<<ValorOro: "+garantia.getValorOro()+">>>");
System.out.println("<<<ValorRealizacion: "+garantia.getValorRealizacion()+">>>");
System.out.println("<<<ValorAplicableCredito: "+garantia.getValorAplicableCredito()+">>>");
