    <bpmn2:scriptTask id="_7C81970E-4C65-4EAB-A1D9-3044B3F66219" name="InicioBucle
PreciosTask" scriptFormat="http://www.java.com/java">
      <bpmn2:extensionElements>
        <drools:metaData name="elementname">
          <drools:metaValue><![CDATA[InicioBucle
PreciosTask]]></drools:metaValue>
        </drools:metaData>
      </bpmn2:extensionElements>
      <bpmn2:incoming>_14C22E62-D6BB-4323-9F36-74D953103502</bpmn2:incoming>
      <bpmn2:outgoing>_4EC7C803-CBED-4836-9D0D-43567FBFA7A1</bpmn2:outgoing>
      <bpmn2:script><![CDATA[//EJEMPO PARA REPLACE Y BUSQUEDA EN LISTA
/*Double valorOnza = 0.0;
valorOnza = Double.parseDouble(Carga.getXmlVariablesInternas().getVariablesInternas().getVariable().stream().filter(variable -> variableValorOnzaTroy.replaceAll(" ","").equals(variable.getNombre().replaceAll(" ",""))).findFirst().orElse(new com.relative.QuskiOro.ObjetosNegocio.Variable(0,"","0.0")).getValor());
*/

//CREAMOS NUEVA INSTANCIA DEL OBJETO DE GARANTIA
garantia = new com.relative.QuskiOro.ObjetosNegocio.Garantia();
kcontext.setVariable("garantia",garantia);

//REINICIAMOS EL INDEX
kcontext.setVariable("index",0);

//INICIALIZAMOS LAS VARIABLES DE NEGOCIO CORRESPONDIENTES AL PROCESO DE CALCULO DE GARANTIAS
variablesNegocio.setPrecioGramo(Carga.getXmlVariablesInternas().getVariablesInternas().getValorOnzaTroy()/((Double)kcontext.getKnowledgeRuntime().getGlobal("GramosPorOnza")));
variablesNegocio.setTotalPrecioOro(0.0);
variablesNegocio.setTotalGarantia(0.0);
variablesNegocio.setTotalPeso(0.0);

//SETAMOS EL COLATERAL ADICIONAL PARA EL CLEINTE
System.out.println("<<<ColateralInterno1: "+cliente.getColateral()+">>>");
if (cliente.getColateralAdicional()==null){cliente.setColateralAdicional(0);}
/*
cliente.setColateral(cliente.getColateral()+cliente.getColateralAdicional());
System.out.println("<<<ColateralInterno2: "+cliente.getColateral()+">>>");
*/
//SI EXISTECOLATERAL EXCEPCIONADA SE REALIZA LOS CALCULOS CON ESA VARIABLE CASO CONTRARIO CON LA VARIABLE CALCULADA
if (cliente.getColateralExcepcionada() != null){
    if (cliente.getColateralExcepcionada()>0){
        cliente.setColateralOriginal(cliente.getColateralCalculoBre()+cliente.getColateralAdicional());
        cliente.setColateral(cliente.getColateralExcepcionada());
    }
    else{
        cliente.setColateral(cliente.getColateral()+cliente.getColateralAdicional());
    }
}
else{
    cliente.setColateral(cliente.getColateral()+cliente.getColateralAdicional());
}
System.out.println("<<<ColateralInterno2: "+cliente.getColateral()+">>>");

System.out.println("<<<INICIO BUCLE PRECIOS TASK>>>");
System.out.println("<<<Colateral adicional: "+cliente.getColateralAdicional()+">>>");]]></bpmn2:script>
    </bpmn2:scriptTask>
